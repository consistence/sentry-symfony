{
	"name": "consistence/sentry-symfony",
	"description": "-",
	"license": "MIT",
	"authors": [
		{
			"name": "Va≈°ek Purchart",
			"email": "me@vasekpurchart.cz",
			"homepage": "http://vasekpurchart.cz"
		}
	],
	"require": {
		"php": "~7.2",
		"consistence/class-finder-implementation": "1.0",
		"consistence/sentry": "~0.15",
		"doctrine/annotations": "~1.2",
		"symfony/config": "~3.0",
		"symfony/console": "~3.0",
		"symfony/dependency-injection": "~3.0",
		"symfony/http-kernel": "~3.0",
		"symfony/yaml": "~3.0"
	},
	"require-dev": {
		"consistence/coding-standard": "3.10.1",
		"consistence/consistence-robot-loader-class-finder": "~0.2",
		"jakub-onderka/php-console-highlighter": "0.4",
		"jakub-onderka/php-parallel-lint": "1.0",
		"matthiasnoback/symfony-dependency-injection-test": "4.2.1",
		"php-parallel-lint/php-console-highlighter": "1.0",
		"php-parallel-lint/php-parallel-lint": "1.3.2",
		"phing/phing": "2.16.1",
		"phpunit/phpunit": "8.5.25"
	},
	"autoload": {
		"psr-4": {
			"Consistence\\Sentry\\SymfonyBundle\\": "src"
		},
		"classmap": [ "src" ]
	},
	"autoload-dev": {
		"psr-4": {
			"Consistence\\Sentry\\SymfonyBundle\\": "tests"
		},
		"classmap": [ "tests" ]
	},
	"config": {
		"bin-dir": "bin",
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": false
		}
	}
}
